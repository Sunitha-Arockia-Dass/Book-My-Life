    <div class="right">
      <div class="embed-page-recipe">
        <div id="vignette">
          <div id="v-container">
            <div class="profile-details">


              <div class="name-age">
                <br>                
                <div class="profile-vignette2">
                  <div id="frame-profile">
                    <div class="profile-image-container">
                      <img class="profile-picture" src="{{profile.profilePicture}}" alt="error" />
                    </div>
                  </div>
                </div>
                <br>
                <h2>Hello, <span class="capitalize">{{profile.name}}</span></h2>
                {{#if isAdult}}
                <p>Already {{age}} Years old! It's good to take care of you.</p>
                {{else}}
                <p>{{age}} Years old! It's good to be young.</p>
                {{/if}}
              </div>


              <div class="profile-detail-btn">
                <form class="form-btn" action="/health/healthDetail/{{profile.id}}" method="get">
                  <button type="submit"><span>Healthify</span></button>
                </form>
                <form class="form-btn" action="/agenda/agenda" method="post">
                  <input type="hidden" name="selectedProfileId" value="{{id}}">
                  <button class="alink" type="submit"><span>Agenda</span></button>
                </form>
              </div>



              <div class="profile-update-delete">
                <article class="text-center">Want to update or delete {{profile.name}}'s profile?</article>
                <div class="profile-btn">


