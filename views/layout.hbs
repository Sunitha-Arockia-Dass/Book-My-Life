<html lang="en">

  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>{{appTitle}}</title>

    <!-- Bootstrap & CSS link -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet"
      integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/5.8.0/main.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/stylesheets/style.css" />

  </head>

  <body>
    <header>
      <nav class="navbar sticky-top navbar-expand-lg">
        <div class="container-fluid">
          <!-- Logo -->
          <div class="text-center">
            <a class="navbar-brand" href="/"><img id="logo-img" src="" alt="Logo" width="auto" height="0"
                class="d-inline-block" /></a>
            <a id="logo-text" class="navbar-brand" href="/">
              <h1>H.E.A.L.<span class="citrine">T.H.</span></h1>
            </a>
          </div>
          <div class="text-start">
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavDropdown"
              aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
          </div>

          <!-- Menus -->
          <div class="collapse navbar-collapse justify-content-end" id="navbarNavDropdown">

            <ul id="is-log-out" class="navbar-nav align-items-center">
              {{#unless isAuthenticated}}
              <li class="nav-item">
                <a class="nav-link alink fs-5" aria-current="page" href="/auth/signup">Signup</a>
              </li>

              <li class="nav-item">
                <a class="nav-link alink fs-5" href="/auth/login">Login</a>
              </li>
              {{/unless}}
            </ul>


            <ul id="is-log-in" class="navbar-nav align-items-center">
              {{#if isAuthenticated}}
              <li class="nav-item">
                <a class="nav-link alink fs-5" href="/profile/profile">Profiles page</a>
              </li>

              <!-- dropdown menu -->
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle alink fs-5" href="/profile/profile" role="button"
                  data-bs-toggle="dropdown" aria-expanded="false">
                  Members
                </a>

                <ul class="dropdown-menu">
                  {{#each profile}}
                  <li class="profile-border"><a class="dropdown-item alink"
                      href="/profile/profileDetail/{{id}}">{{name}}</a></li>
                  {{/each}}
                </ul>
              </li>

              
              {{/if}}
            </ul>
          </div></div>
        

        <div class="collapse navbar-collapse justify-content-end" id="navbarNavDropdown">
          <ul id="is-log-in" class="navbar-nav align-items-center">
            {{#if isAuthenticated}}
            <!--Account button-->

            <li class="nav-item">
              <form class="nav-link fs-5" id="account-form" action="/auth/userProfile" method="GET">
                <button class="Btn">
                  <div class="sign"><svg width="64px" height="64px" viewBox="0 0 24 24" fill="none"
                      xmlns="http://www.w3.org/2000/svg">
                      <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                      <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                      <g id="SVGRepo_iconCarrier">
                        <path
                          d="M17 8C17 10.7614 14.7614 13 12 13C9.23858 13 7 10.7614 7 8C7 5.23858 9.23858 3 12 3C14.7614 3 17 5.23858 17 8Z"
                          stroke="#dda0dd" stroke-width="2"></path>
                        <path d="M3 21C3.95728 17.9237 6.41998 17 12 17C17.58 17 20.0427 17.9237 21 21" stroke="#dda0dd"
                          stroke-width="2" stroke-linecap="round"></path>
                      </g>
                    </svg></div>
                  <div class="text fs-5 alink">Account</div>
                </button>
              </form>
            </li>

            <!--logout button-->

            <li class="nav-item">
              <form class="nav-link fs-5" id="logout-form" action="/auth/logout" method="GET">
                <button class="Btn">
                  <div class="sign"><svg width="64px" height="64px" viewBox="0 0 24 24" fill="none"
                      xmlns="http://www.w3.org/2000/svg">
                      <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                      <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
                      <g id="SVGRepo_iconCarrier">
                        <path d="M21 12L13 12" stroke="#ce78ce" stroke-width="2" stroke-linecap="round"
                          stroke-linejoin="round"></path>
                        <path d="M18 15L20.913 12.087V12.087C20.961 12.039 20.961 11.961 20.913 11.913V11.913L18 9"
                          stroke="#ce78ce" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path>
                        <path
                          d="M16 5V4.5V4.5C16 3.67157 15.3284 3 14.5 3H5C3.89543 3 3 3.89543 3 5V19C3 20.1046 3.89543 21 5 21H14.5C15.3284 21 16 20.3284 16 19.5V19.5V19"
                          stroke="#ce78ce" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path>
                      </g>
                    </svg>
                    <div class="text fs-5 alink">Logout</div>
                </button>
              </form>
            </li>
            {{/if}}
          </ul>
        </div>
      
      </nav>
    </header>

    <main>
      <!-- Navigation Buttons -->
      <section class="navigation">
        <nav class="back-Forward">
          <button onclick="history.back()"><svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
              <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
              <g id="SVGRepo_iconCarrier">
                <path opacity="0"
                  d="M17.4 8L17.5867 7.88333C18.2019 7.4988 19 7.94112 19 8.66667L19 15.3333C19 16.0589 18.2019 16.5012 17.5867 16.1167L17.4 16L12.6 13C11.861 12.5381 11.861 11.4619 12.6 11L17.4 8Z"
                  fill="#ead94c"></path>
                <path
                  d="M17.4 8L17.5867 7.88333C18.2019 7.4988 19 7.94112 19 8.66667L19 15.3333C19 16.0589 18.2019 16.5012 17.5867 16.1167L17.4 16L12.6 13C11.861 12.5381 11.861 11.4619 12.6 11L17.4 8Z"
                  stroke="#963696" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
              </g>
            </svg></button>
          <button onclick="history.forward()"><svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
              <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
              <g id="SVGRepo_iconCarrier">
                <path opacity="0"
                  d="M6.6 8L6.41333 7.88333C5.79807 7.4988 5 7.94112 5 8.66667L5 15.3333C5 16.0589 5.79807 16.5012 6.41333 16.1167L6.6 16L11.4 13C12.139 12.5381 12.139 11.4619 11.4 11L6.6 8Z"
                  fill="#ead94c"></path>
                <path
                  d="M6.6 8L6.41333 7.88333C5.79807 7.4988 5 7.94112 5 8.66667L5 15.3333C5 16.0589 5.79807 16.5012 6.41333 16.1167L6.6 16L11.4 13C12.139 12.5381 12.139 11.4619 11.4 11L6.6 8Z"
                  stroke="#963696" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
              </g>
            </svg></button>
        </nav>
      </section>

      {{{body}}}

      <!-- FOOTER -->
      <footer>
        <div id="footer" class="text-sm-start text-md-start">
          <p><a href="https://www.linkedin.com/in/sunitha-arockia-dass-587b8679/" target="_blank"
              rel="noopener noreferrer">Sunitha</a>
            and
            <a href="https://www.linkedin.com/in/thomas-lebas26/" target="_blank" rel="noopener noreferrer">Thomas</a>
            development & design, all right reserved <sup>Â©</sup>
          </p>
        </div>
      </footer>
    </main>




    <!-- Bootstrap script -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4"
      crossorigin="anonymous"></script>
    <!-- JavaScript script -->
    
    <script src="/js/test.js"></script>
    <script src="/js/script.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.7.0/chart.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.7.0/chart.min.js"></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/chartjs-plugin-fillbetween/0.7.0/chartjs-plugin-fillbetween.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-plugin-datalabels"></script>
  </body>

</html>